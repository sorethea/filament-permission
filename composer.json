{
    "name": "sorethea/permission",
    "description": "Filament plugin for permissions and roles",
    "type": "filament-plugin",
    "require": {
        "spatie/laravel-permission": "5.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sorethea\\Permission\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sorethea Ok",
            "email": "sorethea@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": [],
            "provider": [
                "Sorethea\\Permission\\PermissionServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev"
}
